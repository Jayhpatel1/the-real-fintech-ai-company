{
  "hosting": [
    {
      "target": "therealfintech",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=3600"
            },
            {
              "key": "X-Frame-Options",
              "value": "DENY"
            },
            {
              "key": "X-Content-Type-Options",
              "value": "nosniff"
            }
          ]
        }
      ],
      "redirects": [
        {
          "source": "/therealfintech.com",
          "destination": "/",
          "type": 301
        },
        {
          "source": "/redirect",
          "destination": "/",
          "type": 301
        }
      ]
    },
    {
      "target": "main",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=3600"
            },
            {
              "key": "X-Frame-Options",
              "value": "DENY"
            },
            {
              "key": "X-Content-Type-Options",
              "value": "nosniff"
            }
          ]
        }
      ],
      "redirects": [
        {
          "source": "/therealfintech.com",
          "destination": "/",
          "type": 301
        },
        {
          "source": "/redirect",
          "destination": "/",
          "type": 301
        }
      ]
    }
  ]
}
